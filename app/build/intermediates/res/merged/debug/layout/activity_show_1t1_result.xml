<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3A006F"
    android:padding="@dimen/distance_30dp"
    >
    <!--超时 布局-->
    <include
        android:id="@+id/mCompareRresult_OutOfTime_ly"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        layout="@layout/read_idcard_failure"

        android:visibility="gone"
        />
    <LinearLayout
        android:id="@+id/mCompareRresult_ly"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <!--标题-->
        <LinearLayout
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="@dimen/distance_45dp"
            android:background="@drawable/top_round_blue"
            >
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="人证对比信息"
                android:textColor="@color/white"
                android:gravity="center"
                android:textSize="18dp"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text="访客证件信息"
                android:textColor="@color/white"
                android:gravity="center"
                android:textSize="18dp"
                />
        </LinearLayout>
        <!--标题下-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/round_buttom_white"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="7"
                    android:layout_height="0dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_gravity="center"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center"
                        >
                        <ImageView
                            android:id="@+id/id_card_face2"
                            android:layout_width="@dimen/distance_120dp"
                            android:layout_height="@dimen/distance_120dp"
                            android:src="@drawable/ic_launcher"
                            android:scaleType="fitXY"
                            />
                        <ImageView
                            android:id="@+id/succeed_or_failure_img"
                            android:layout_width="@dimen/distance_55dp"
                            android:layout_height="@dimen/distance_55dp"
                            android:src="#000"
                            android:layout_marginRight="@dimen/distance_35dp"
                            android:layout_marginLeft="@dimen/distance_35dp"
                            android:layout_marginTop="@dimen/distance_5dp"
                            android:layout_marginBottom="@dimen/distance_5dp"
                            />
                        <!--抓拍人脸-->
                        <ImageView
                            android:id="@+id/site_face_image"
                            android:layout_width="@dimen/distance_120dp"
                            android:layout_height="@dimen/distance_120dp"
                            android:src="@drawable/ic_launcher"
                            android:scaleType="fitXY"
                            />
                    </LinearLayout>
                    <TextView
                        android:id="@+id/succeed_or_failure_text"
                        android:layout_marginTop="@dimen/distance_35dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="验证结果"
                        android:textSize="@dimen/distance_25dp"
                        android:textStyle="bold"
                        android:textColor="#ff0000"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="3"
                    android:layout_height="0dp"
                    android:orientation="vertical"
                    android:background="@drawable/round_hui_ef"
                    android:layout_marginRight="@dimen/distance_55dp"
                    android:layout_marginLeft="@dimen/distance_55dp"
                    android:layout_marginBottom="@dimen/distance_25dp"
                    >
                    <TextView
                        android:id="@+id/textScore"
                        android:layout_marginTop="@dimen/distance_15dp"
                        android:layout_marginLeft="@dimen/distance_80dp"
                        android:layout_marginRight="@dimen/distance_80dp"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/distance_25dp"
                        android:background="@drawable/round_white"
                        android:text="对比结果："
                        android:paddingLeft="@dimen/distance_5dp"
                        android:paddingRight="@dimen/distance_5dp"
                        />
                    <TextView
                        android:id="@+id/textResult"
                        android:layout_marginTop="@dimen/distance_15dp"
                        android:layout_marginLeft="@dimen/distance_80dp"
                        android:layout_marginRight="@dimen/distance_80dp"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/distance_25dp"
                        android:background="@drawable/round_white"
                        android:text="验证结果："
                        android:paddingLeft="@dimen/distance_5dp"
                        android:paddingRight="@dimen/distance_5dp"
                        />
                    <TextView
                        android:id="@+id/textTime"
                        android:layout_marginTop="@dimen/distance_15dp"
                        android:layout_marginLeft="@dimen/distance_80dp"
                        android:layout_marginRight="@dimen/distance_80dp"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/distance_25dp"
                        android:background="@drawable/round_white"
                        android:layout_gravity="center"
                        android:text="对比时间："
                        android:paddingLeft="@dimen/distance_5dp"
                        android:paddingRight="@dimen/distance_5dp"
                        />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_weight="6"
                    android:layout_height="0dp"
                    android:orientation="vertical"
                    android:background="@drawable/auth_right2"
                    android:layout_marginRight="@dimen/distance_25dp"
                    android:layout_marginTop="@dimen/distance_25dp"
                    >
                    <!--  android:background="@drawable/round_hui_ef"
                     android:background="@drawable/round_white"
                    -->
                    <ImageView
                        android:id="@+id/id_card_face"
                        android:layout_alignParentRight="true"
                        android:layout_marginTop="@dimen/distance_20dp"
                        android:layout_marginRight="@dimen/distance_55dp"
                        android:layout_width="@dimen/distance_120dp"
                        android:layout_height="@dimen/distance_100dp"
                        android:src="#ff00ff"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        >
                        <TextView
                            android:id="@+id/id_card_name"
                            android:layout_marginTop="@dimen/distance_25dp"
                            android:layout_marginLeft="@dimen/distance_25dp"
                            android:layout_marginRight="@dimen/distance_10dp"
                            android:layout_width="@dimen/distance_100dp"
                            android:layout_height="wrap_content"
                            android:text="姓名：程情"
                            android:textSize="@dimen/distance_15dp"
                            />
                        <TextView
                            android:id="@+id/id_card_sex"
                            android:layout_marginTop="@dimen/distance_10dp"
                            android:layout_marginLeft="@dimen/distance_25dp"
                            android:layout_marginRight="@dimen/distance_10dp"
                            android:layout_width="@dimen/distance_100dp"
                            android:layout_height="wrap_content"
                            android:text="性别：女"
                            android:textSize="@dimen/distance_15dp"
                            />
                        <TextView
                            android:id="@+id/id_card_nation"
                            android:layout_marginTop="@dimen/distance_10dp"
                            android:layout_marginLeft="@dimen/distance_25dp"
                            android:layout_marginRight="@dimen/distance_10dp"
                            android:layout_width="@dimen/distance_100dp"
                            android:layout_height="@dimen/distance_25dp"
                            android:gravity="center|left"
                            android:text="民族：汉"
                            android:textSize="@dimen/distance_15dp"
                            />
                        <TextView
                            android:id="@+id/id_card_dateOfBirth"
                            android:layout_marginTop="@dimen/distance_10dp"
                            android:layout_marginLeft="@dimen/distance_25dp"
                            android:layout_marginRight="@dimen/distance_10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="出生年月：的梵蒂冈梵蒂冈"
                            android:textSize="@dimen/distance_15dp"
                            />
                        <TextView
                            android:id="@+id/id_card_Address"
                            android:layout_marginTop="@dimen/distance_10dp"
                            android:layout_marginLeft="@dimen/distance_25dp"
                            android:layout_marginRight="@dimen/distance_10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="家庭住址：的梵蒂冈梵蒂冈"
                            android:textSize="@dimen/distance_15dp"
                            />
                        <TextView
                            android:id="@+id/id_card_number"
                            android:layout_marginTop="@dimen/distance_10dp"
                            android:layout_marginLeft="@dimen/distance_25dp"
                            android:layout_marginRight="@dimen/distance_10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="身份证号：的梵蒂冈梵蒂冈"
                            android:textSize="@dimen/distance_15dp"
                            />
                        <TextView
                            android:id="@+id/id_card_ValidTime"
                            android:layout_marginTop="@dimen/distance_10dp"
                            android:layout_marginLeft="@dimen/distance_25dp"
                            android:layout_marginRight="@dimen/distance_10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="有效期：的梵蒂冈梵蒂冈"
                            android:textSize="@dimen/distance_15dp"
                            />
                    </LinearLayout>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_weight="4"
                    android:layout_height="0dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    >
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/distance_35dp"
                        android:orientation="horizontal"
                        android:background="@drawable/round_blue_15dp"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:padding="@dimen/distance_5dp"

                        android:visibility="gone"
                        >
                        <ImageView
                            android:layout_width="@dimen/distance_25dp"
                            android:layout_height="@dimen/distance_25dp"
                            android:src="@drawable/internet"
                            android:layout_marginRight="@dimen/distance_5dp"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="保存信息"
                            android:textSize="@dimen/distance_15dp"
                            android:textColor="@color/white"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="@dimen/distance_15dp"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/distance_35dp"
                        android:orientation="horizontal"
                        android:background="@drawable/round_orange_15dp"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:padding="@dimen/distance_5dp"

                        android:visibility="gone"
                        >
                        <ImageView
                            android:layout_width="@dimen/distance_25dp"
                            android:layout_height="@dimen/distance_25dp"
                            android:src="@drawable/internet"
                            android:layout_marginRight="@dimen/distance_5dp"
                            />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="注册入网"
                            android:textSize="@dimen/distance_15dp"
                            android:textColor="@color/white"
                            />
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>
